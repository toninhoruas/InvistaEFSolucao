@model IEnumerable<InvistaEFSolucao.Models.IndicadorBiologico>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Produto)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Microorganismo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CorOriginal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ValorD)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ValorZ)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ValorN)
